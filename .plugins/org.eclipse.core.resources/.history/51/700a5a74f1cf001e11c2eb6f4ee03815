package restAssured7;
import static io.restassured.RestAssured.given;

import org.testng.annotations.Test;
public class DeleteUsers {

	
	@Test
	void deleteUsers() {
		String token = "9f378887517c46948b9c0e768423abb96c0dcf03b61c7b3bd40af81f74d8ff6d";
		int id=0;
		given().header("Authorization", "Bearer " + token).pathParam("ID", id)
		.when().delete("https://gorest.co.in/public/v2/users/{ID}")
		.then().statusCode(204).log().all();
		
		
	}
}
